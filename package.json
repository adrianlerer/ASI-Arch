{
  "name": "@asi-arch/meta",
  "version": "0.1.0",
  "description": "Meta-agent control center for ASI Architecture ecosystem",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "test": "jest",
    "lint": "eslint src/**/*.ts",
    "format": "prettier --write src/**/*.ts",
    "dev": "ts-node src/index.ts",
    "init": "node scripts/init-ecosystem.js",
    "evolve": "node scripts/evolution-loop.js",
    "monitor": "node scripts/monitor-ecosystem.js",
    "sync:registry": "node scripts/sync-registry.js",
    "eval:agents": "node scripts/evaluate-agents.js",
    "docs": "typedoc src --out docs/api"
